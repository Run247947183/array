class Solution {
    public int majorityElement(int[] nums) {
        int midNum = nums.length / 2;
        Map<Integer, Integer> map = new HashMap<>();
        int res = 0;
        for (int num : nums) {
            map.put(num, map.getOrDefault(num, 0) + 1);
        }
        for (int num2 : nums) {
            if (map.get(num2) > midNum) {
                res = num2;
            }
        }
        return res;
    }
}
